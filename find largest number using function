num1=int(input ("enter first number"))
num2=int(input ("enter second number"))
num3=int(input ("enter third number"))

def find (a,b,c):
    if a>b:
        if a > c:
            return a
        else:
            return c
    if b>a:
        if b>c:
            return b
        else:
            return c
result=(find(num1,num2,num3))
print("the largest number is",result)





